                                 x:Name="WeekDay3"
                                 Foreground="{TemplateBinding CalendarItemForeground}"
                                 Style="{StaticResource WeekDayNameStyle}" SelectionHighlightColor="#CCFFCB46"/>
                                        <TextBlock Grid.Column="3"
