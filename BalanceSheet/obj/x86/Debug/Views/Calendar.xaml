<Page x:ConnectionId='1'
    x:Class="BalanceSheet.Views.Calendar"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:BalanceSheet.Views"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    x:Name="pageRoot" Margin="0,-23,0,0">

    <Grid Background="#FF119DDA" Margin="0">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>

        </Grid.RowDefinitions>
        <Grid Grid.Row="0" HorizontalAlignment="Stretch"
              VerticalAlignment="Top"
              Margin="5,45,5,0">
            <TextBlock x:Uid="MonatWahl" Foreground="White" 
                       Margin="5" HorizontalAlignment="Center"
                       FontSize="16" FontFamily="Segoe Print"/>

        </Grid>
        <Grid Grid.Row="1" HorizontalAlignment="Stretch"
              VerticalAlignment="Stretch">
            <DatePicker x:ConnectionId='14' x:Name="UserDatePicker" Grid.Row="0" 
                        Grid.ColumnSpan="3"
                        HorizontalAlignment="Stretch" 
                        VerticalAlignment="Stretch"
                        Height="50"
                        Margin="5"
                        Foreground="White"
                        Background="#FF45B8EA"
                        FontSize="16"
                        Style="{StaticResource DatePickerUserStyle3}" 
                        DayVisible="False" MonthVisible="False" 
                                                                 
                                                       
                                                           
                        FontFamily="Segoe Print"                                
                        >
            </DatePicker>
        </Grid>
        <Grid Grid.Row="2" HorizontalAlignment="Stretch"
              VerticalAlignment="Stretch"
              Margin="0,0.5">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Button x:ConnectionId='2' x:Name="BtnJanuar" Grid.Column="0" Grid.Row="0" 
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch" Foreground="White" 
                    FontSize="16" Background="#FF45B8EA" Margin="5,5,1,1"
                    Style="{StaticResource UserButtonStyle}" FontFamily="Segoe Print"                        />
            <Button x:ConnectionId='3' x:Name="BtnFebruar" Grid.Column="1" Grid.Row="0" 
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch" Foreground="White" 
                    FontSize="16" Background="#FF45B8EA" Margin="1,5,1,1"
                    Style="{StaticResource UserButtonStyle}" FontFamily="Segoe Print"                         />
            <Button x:ConnectionId='4' x:Name="BtnMarz" Grid.Column="2" Grid.Row="0" 
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch" Foreground="White" 
                    FontSize="16" Background="#FF45B8EA" Margin="1,5,5,1"
                    Style="{StaticResource UserButtonStyle}" FontFamily="Segoe Print"                      />
            <Button x:ConnectionId='5' x:Name="BtnApril" Grid.Column="0" Grid.Row="1" 
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch" Foreground="White" 
                    FontSize="16" Background="#FF45B8EA" Margin="5,1,1,1"
                    Style="{StaticResource UserButtonStyle}" FontFamily="Segoe Print"                       />
            <Button x:ConnectionId='6' x:Name="BtnMai" Grid.Column="1" Grid.Row="1" 
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch" Foreground="White" 
                    FontSize="16" Background="#FF45B8EA" Margin="1"
                    Style="{StaticResource UserButtonStyle}" FontFamily="Segoe Print"                     />
            <Button x:ConnectionId='7' x:Name="BtnJuni" Grid.Column="2" Grid.Row="1" 
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch" Foreground="White" 
                    FontSize="16" Background="#FF45B8EA" Margin="1,1,5,1"
                    Style="{StaticResource UserButtonStyle}" FontFamily="Segoe Print"                      />
            <Button x:ConnectionId='8' x:Name="BtnJuli" Grid.Column="0" Grid.Row="2" 
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch" Foreground="White" 
                    FontSize="16" Background="#FF45B8EA" Margin="5,1,1,1"
                    Style="{StaticResource UserButtonStyle}" FontFamily="Segoe Print"                      />
            <Button x:ConnectionId='9' x:Name="BtnAugust" Grid.Column="1" Grid.Row="2" 
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch" Foreground="White" 
                    FontSize="16" Background="#FF45B8EA" Margin="1"
                    Style="{StaticResource UserButtonStyle}" FontFamily="Segoe Print"                        />
            <Button x:ConnectionId='10' x:Name="BtnSeptember" Grid.Column="2" Grid.Row="2" 
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch" Foreground="White" 
                    FontSize="16" Background="#FF45B8EA" Margin="1,1,5,1"
                    Style="{StaticResource UserButtonStyle}" FontFamily="Segoe Print"                           />
            <Button x:ConnectionId='11' x:Name="BtnOktober" Grid.Column="0" Grid.Row="3" 
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch" Foreground="White" 
                    FontSize="16" Background="#FF45B8EA" Margin="5,1,1,5"
                    Style="{StaticResource UserButtonStyle}" FontFamily="Segoe Print"                         />
            <Button x:ConnectionId='12' x:Name="BtnNovember" Grid.Column="1" Grid.Row="3"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch" Foreground="White" 
                    FontSize="16" Background="#FF45B8EA" Margin="1,1,1,5"
                    Style="{StaticResource UserButtonStyle}" FontFamily="Segoe Print"                          />
            <Button x:ConnectionId='13' x:Name="BtnDezember" Grid.Column="2" Grid.Row="3" 
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch" Foreground="White" 
                    FontSize="16" Background="#FF45B8EA" Margin="1,1,5,5"
                    Style="{StaticResource UserButtonStyle}" FontFamily="Segoe Print"                          />
        </Grid>
    </Grid>
</Page>

